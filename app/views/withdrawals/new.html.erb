<%= form_with model: @withdraw, url: withdrawals_path do |form| %>
  <p>
    <%= form.label :id_number %><br>
    <%= form.text_field :id_number %>
  </p>
  <p>
    <%= form.label :amount %><br>
    <%= form.text_field :amount %>
  </p>
  <p>
    <%= form.label :pin %><br>
    <%= form.text_field :pin %>
  </p>
  <p>
    <%= form.submit "Withdraw" %>
  </p>
<% end %>