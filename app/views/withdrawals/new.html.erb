 <%= form_with scope: :withdraw, url: new_user_path, local: true do |form| %>
 <p>
    <%= form.label :id_number %><br>
    <%= form.text_field :id_number %>
  </p>
  <p>
    <%= form.label :amount %><br>
    <%= form.text_field :amount %>
  </p>
  <p>
    <%= form.label :pin %><br>
    <%= form.text_field :pin %>
  </p>
  <p>
    <%= form.submit "Withdraw" %>
  </p>
  <% end %>